{
  "compilerOptions": {
    "target": "Es2022",
    "module": "ES2022",

    "allowJs": true,
    "declaration": true,
    "outDir": "./dist",
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["./*.ts"],
  "exclude": ["./build-wasm/**"]
}
